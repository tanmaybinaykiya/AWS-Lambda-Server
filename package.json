{
  "name": "secure-slice-backend",
  "version": "0.0.1",
  "description": "REST API server for secure slice app",
  "author": "Tanmay Binaykiya",
  "scripts": {
    "start": "./node_modules/grunt/bin/grunt  && node bin/www",
    "grunt": "./node_modules/grunt/bin/grunt ",
    "prepareForDeploy": "npm install && ./node_modules/grunt/bin/grunt cleanDeploy && cd functions && npm install --production",
    "prepareForDeployNoInstall": "./node_modules/grunt/bin/grunt cleanDeploy && cd functions && npm install --production",
    "deployToAws": "cd functions && AWS_CLIENT_TIMEOUT=1200000 SLS_DEBUG=\"asd\" ../node_modules/serverless/bin/serverless"
  },
  "dependencies": {
    "cloudflare": "1.1.1",
    "co": "4.6.0",
    "co-bcryptjs": "0.2.0",
    "dotenv": "2.0.0",
    "dynogels": "6.1.0",
    "email-templates": "2.3.0",
    "joi": "10.1.0",
    "jsonwebtoken": "7.1.9",
    "koa": "1.2.4",
    "koa-better-router": "2.0.1",
    "koa-body": "1.6.0",
    "koa-cors": "0.0.16",
    "koa-jwt": "1.2.0",
    "node-uuid": "1.4.7",
    "nodemailer": "2.4.2",
    "nodemailer-sendgrid-transport": "0.2.0",
    "request": "2.75.0",
    "requestretry": "1.12.0",
    "serverless-plugin-stage-variables": "1.6.6",
    "serverless-plugin-write-env-vars": "1.0.1",
    "uuid": "2.0.3",
    "vogels": "2.2.0"
  },
  "devDependencies": {
    "aws-sdk": "2.6.7",
    "grunt": "1.0.1",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-watch": "1.0.0",
    "serverless": "1.0.2",
    "lodash": "4.17.2"
  }
}